<!-- <div class="container"> -->
  <div class="container sign-wrapper text-center">
  <div class="row">
    <div class="col-xs-12 col-md-offset-3 col-md-6">
      <%= simple_form_for(@flat) do |f| %>
        <%= f.input :name %>
        <%= f.input :description %>
        <%= f.input :location %>
        <%= f.input :price %>
        <%= f.input :photo %>
        <%= f.input :photo_cache, as: :hidden %>
        <%= f.submit class: "btn-airbnb-2"%>
      <% end %>
    </div>
  </div>
</div>
